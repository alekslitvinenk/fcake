var doc = fl.getDocumentDOM();var selects = doc.selection;if(selects.length){		var libItems = doc.library.getSelectedItems();		if(libItems.length){				var itemToPaste = libItems[0];		var layerName = itemToPaste.name;				var tim = doc.getTimeline();				var createLayer = true;				for(var i = 0; i < tim.layers.length; i++){			if(tim.layers[i].name == layerName){				createLayer = false;				break;			}		}				tim.currentLayer = 0;				if(createLayer){			tim.addNewLayer(layerName);		}				var layerIndex = tim.findLayerIndex(layerName)[0];				tim.currentLayer = layerIndex;				for(var j = 0; j < selects.length; j++){			doc.addItem({x: selects[j].transformX, y:selects[j].transformY}, itemToPaste);		}			}else{		alert("You didn't choose any library item!");	}	}else{	alert("You didn't selected any element!");}