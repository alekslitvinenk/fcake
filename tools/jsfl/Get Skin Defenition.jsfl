var doc = fl.getDocumentDOM();var curTimeline = doc.getTimeline();var skinParts = [];for(var l = 0; l < curTimeline.layers.length; l++){		var layer = curTimeline.layers[l].name;		var elements = curTimeline.layers[l].frames[curTimeline.currentFrame].elements;		for(var i = 0; i < elements.length; i++)	{		var elem = elements[i];				if(elem.name && elem.elementType == "instance")		{			//fl.trace(elem.name + " " + elem.instanceType);						if(elem.instanceType == "symbol")			{				skinParts.push(elem.name);								var libItem = elem.libraryItem;								var def = "private var " + elem.name + ":";								if(libItem.linkageExportForAS)				{					def += libItem.linkageClassName + ";";				}else				{					def += "MovieClip;";				}				fl.trace(def);			}		}	}}fl.trace("");for(var i = 0; i < skinParts.length; i++){	var skinPart = skinParts[i];		fl.trace(skinPart + " = " + "skin[\"" + skinPart + "\"];");}